{contentType application/xml}
<?xml version="1.0" encoding="UTF-8"?>

<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
	<url>
		<loc>{link //Homepage:default}</loc>
		<priority>1.0</priority>
	</url>
	<url>
		<loc>{link //Single:about}</loc>
		<priority>0.9</priority>
	</url>
	<url>
		<loc>{link //Single:develop}</loc>
		<priority>0.9</priority>
	</url>
	<url>
		<loc>{link //Single:obsah}</loc>
		<priority>0.9</priority>
	</url>
	<url>
		<loc>{link //Single:reference}</loc>
		<priority>0.9</priority>
	</url>
	{foreach $sitemap as $s}
		<url>
			<loc>{link //Single:article $s->slug}</loc>
			<priority>0.8</priority>
		</url>
	{/foreach}
</urlset>
